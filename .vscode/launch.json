    "version": "0.2.0",
    "go.alternateTools": {
        "go": "/opt/homebrew/bin/go"
    },
    "configurations": [

        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}/mocktail-api/main.go"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        },
        
    ]
}